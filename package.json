{
  "name": "poddashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -c \"#f59e0b,#0ea5e9\" \"\\\"$npm_execpath\\\" run dev:api\" \"nuxt dev --port 3000\" --names=api,web",
    "dev:api": "\"$npm_execpath\" run --watch backend/index.ts",
    "dev:web": "nuxt dev --port 3000",
    "build": "nuxt build",
    "start": "concurrently -c \"#f59e0b,#0ea5e9\" \"\\\"$npm_execpath\\\" run start:api\" \"\\\"$npm_execpath\\\" run start:web\" --names=api,web",
    "start:api": "\"$npm_execpath\" run backend/index.ts",
    "start:web": "\"$npm_execpath\" .output/server/index.mjs",
    "lint": "biome check .",
    "format": "biome format --write .",
    "smoke": "\"$npm_execpath\" run scripts/smoke.ts",
    "typecheck": "nuxi typecheck",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "\"$npm_execpath\" run db/migrate.ts",
    "db:seed": "\"$npm_execpath\" run db/seed.ts",
    "scry:doctor": "\"$npm_execpath\" run scripts/cli.ts doctor"
  },
  "dependencies": {
    "@auth/core": "^0.41.0",
    "@auth/drizzle-adapter": "^1.10.0",
    "@nuxt/ui": "^4.1.0",
    "bcryptjs": "^3.0.2",
    "concurrently": "^9.2.1",
    "drizzle-orm": "^0.44.5",
    "nuxt": "^4.1.0",
    "pg": "^8.16.3",
    "tailwindcss": "^4.1.12",
    "vue": "^3.5.0",
    "vue-router": "^4.5.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/bun": "^1.2.22",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4",
    "typescript": "^5.9.2"
  }
}
