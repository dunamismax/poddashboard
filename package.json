{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev:vite": "vite",
        "dev:server": "php artisan serve",
        "dev:queue": "php artisan queue:listen --tries=1 --timeout=0",
        "dev:logs": "php artisan pail --timeout=0",
        "dev": "concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"bun run dev:server\" \"bun run dev:queue\" \"bun run dev:logs\" \"bun run dev:vite\" --names=server,queue,logs,vite --kill-others"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.11",
        "autoprefixer": "^10.4.20",
        "axios": "^1.7.4",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0",
        "tailwindcss": "^4.0.7",
        "vite": "^7.0.4"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.9.5",
        "@tailwindcss/oxide-linux-x64-gnu": "^4.0.1",
        "lightningcss-linux-x64-gnu": "^1.29.1"
    }
}
